        <div class ="content">
            <div class = "row-sm">
                <div class="col-sm-12">
                    <div class="card-header">
                        <div class="card-body">
                            <div class ="content">
                                <h6> Edit Record</h6>
                                <form action="../views/table_search.ejs" method="post" id= "accounts_form_edit" autocomplete="="off>
                                <div id="tabs">
                                    <ul class="nav nav-pills ul_col" id="tabs">
